                    </div>
                    <div class="suggestions" tagId="suggestions"></div>
                    </ul>
                    <div id="hmvi_explorer_selected_metros" tagId="hmvi_explorer_itme_selected" class="hmvi_explorer_search_items_scroll"></div>
                  </fieldset>
                </div>
                <div class="modal-footer">
                  <button class="btn btn-info btn-block close-btn" type="button" data-dismiss="modal">Close</button>
                </div>
              </div>
            </div>
          
            <div tagId="sort-modal" class="modal">
              <!-- Modal content -->
