                  <h4 class="modal-title">Search</h4>
                  <button class="close" type="button" data-dismiss="modal">×</button>
                </div>
                <div class="modal-body">
                  <fieldset>
                    <div class="hmvi_explorer_autocomplete">
                      <div class="input-group" style="width: 100%;"> <input id="hmvi_explorer_metro_names_input_text" tagId="hmvi_explorer_input_text"
                          class="form-control" type="text" placeholder="Enter metro name" aria-describedby="basic-addon2"
                          autofocus> <span id="basic-addon2" class="input-group-addon"> <span
                            class="pull-right- fa fa-undo hmvi_explorer_refresh_value" tagId="input_text_reset" value="hmvi_explorer_reset_metro_names"
