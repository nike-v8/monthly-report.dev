          <!-- State Modals Start -->
          <div id="modalss">
            <div tagId="search-modal" class="modal">
              <div class="modal-content">
                <div class="modal-header">
                  <h4 class="modal-title">Search</h4>
                  <button class="close" type="button" data-dismiss="modal">×</button>
                </div>
                <div class="modal-body">
                  <fieldset>
                    <div class="hmvi_explorer_autocomplete">
                      <div class="input-group" style="width: 100%;"> <input id="hmvi_explorer_state_names_input_text" tagId="hmvi_explorer_input_text"
                          class="form-control" type="text" placeholder="Enter state name" aria-describedby="basic-addon2"
                          autofocus> <span id="refresh-search" class="input-group-addon" tagId="input_text_reset" > <span
                            class="pull-right- fa fa-undo hmvi_explorer_refresh_value"value="hmvi_explorer_reset_state_names"
                            aria-hidden="true" data-toggle="tooltip" data-placement="bottom" title="Clear"></span> </span></div>
                    </div>
                    <div class="suggestions" tagId="suggestions"></div>
                    </ul>
                    <div id="hmvi_explorer_selected_state" tagId="hmvi_explorer_itme_selected" class="hmvi_explorer_search_items_scroll"></div>
                  </fieldset>
                </div>
                <div class="modal-footer">
                  <button class="btn btn-info btn-block close-btn" type="button" data-dismiss="modal">Close</button>
                </div>
              </div>
            </div>
          
            <div tagId="sort-modal" class="modal">
              <!-- Modal content -->
              <div class="modal-content">
                <div class="modal-header">
                  <h4 class="modal-title">Sort By</h4>
                  <button class="close" type="button" data-dismiss="modal">×</button>
                </div>
                <div class="modal-body">
                  <div class="container-fluid hmvi_explorer_center_filters">
                    <div id="hmvi_explorer_data_set_id" class="btn-group btn-group-justified" data-toggle="buttons">
                      <label id="market-rank" tagId="market_performance" class="btn btn-group-xs btn-default active">
                        <input id="option1" class="hmvi_explorer_data_set_radio" name="options" type="radio"
                          value="score_pctl">Market Rank
                      </label>
                      <label id="situation" tagId="situation" class="btn btn-group-xs btn-default">
                        <input id="option2" class="hmvi_explorer_data_set_radio" name="options" type="radio"
                          value="situation_pctl">Situation
                      </label>
                      <label id="outlook" tagId="outlook" class="btn btn-group-xs btn-default">
                        <input id="option3" class="hmvi_explorer_data_set_radio" name="options" type="radio"
                          value="outlook_pctl">Outlook
                      </label>
                    </div>
                  </div>
                </div>
                <div class="modal-footer">
                  <button class="btn btn-info btn-block close-btn" type="button" data-dismiss="modal">Close</button>
                </div>
              </div>
            </div>
          
            <div tagId="filter-modal" class="modal">
              <!-- Modal content -->
              <div class="modal-content">
                <div class="modal-header">
                  <h4 class="modal-title">Filter</h4>
                  <button class="close" type="button" data-dismiss="modal">×</button>
                </div>
                <div class="modal-body">
                  <div class="market-rank">
                    <div class="filter-title">
                      Market Rank
                    </div>
                    <div class="range-slider" tagId="market-rank-range">
                      <input value="0" min="0" max="100" step="1" type="range" class="slider slider-n">
                      <input value="100" min="0" max="100" step="1" type="range" class="slider slider-x">
                      <div class="slider-values">
                        <input id="slider-min" type="number" value="0" min="0" disabled class="slider-value" tagId="slider-min">
                        <input id="slider-max" type="number" value="100" min="0" disabled class="slider-value" tagId="slider-max">
                      </div>
                    </div>
                  </div>
                  <div class="market-rank">
                    <div class="filter-title" style="margin-top: 10px;">
                      Situation
                    </div>
                    <div class="situation">
                      <label class="switch">
                        <input type="checkbox" checked id="outperforming" tagId="outperforming">
                        <span class="slider round"></span>
                        <label class="switch-label">Outperforming</label>
                      </label>
                      <label class="switch">
                        <input type="checkbox" checked id="tracking" tagId="tracking">
                        <span class="slider round"></span>
                        <label class="switch-label">Tracking</label>
                      </label>
                      <label class="switch">
                        <input type="checkbox" checked id="underperforming" tagId="underperforming">
                        <span class="slider round"></span>
                        <label class="switch-label">Underperforming</label>
                      </label>
                    </div>
                  </div>
                  <div class="market-rank">
                    <div class="filter-title" style="margin-top: 10px;">
                      Outlook
                    </div>
                    <div class="situation">
                      <label class="switch">
                        <input type="checkbox" checked id="outperform" tagId="outperform">
                        <span class="slider round"></span>
                        <label class="switch-label">Outperform</label>
                      </label>
                      <label class="switch">
                        <input type="checkbox" checked id="track" tagId="track">
                        <span class="slider round"></span>
                        <label class="switch-label">Track</label>
                      </label>
                      <label class="switch">
                        <input type="checkbox" checked id="underperform" tagId="underperform">
                        <span class="slider round"></span>
                        <label class="switch-label">Underperform</label>
                      </label>
                    </div>
                  </div>
                </div>
                <div class="modal-footer">
